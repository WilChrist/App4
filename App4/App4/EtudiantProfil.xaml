<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App4.EtudiantProfil">
    <ContentPage.Content>
        <StackLayout>
            <Image x:Name="image"></Image>
            <Label x:Name="nom"></Label>
            <Label x:Name="adresse"></Label>
            <Label x:Name="prenom"></Label>
            <Label x:Name="cne"></Label>
            <Label x:Name="tel"></Label>
            <Label x:Name="sexe"></Label>
            <Label x:Name="filiere"></Label>
            <DatePicker x:Name="date"></DatePicker>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>